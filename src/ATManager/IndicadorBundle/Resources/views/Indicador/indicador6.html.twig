{% extends 'IndicadorBundle::baseindic.html.twig' %} 
{% block content %}        
        <h3>Costos Insumidos en Pesos</h3>	
        {{ form(form)}}
        <br/>
        <table class="table table-bordered table-striped ">
            <thead>
                <th>Nro. Servicio Tercero</th>
                <th>Nombre</th>
                <th>Precio</th>
            </thead>
            <tbody>
                {% for dato in entities.datos%}
                <tr>
                    <td>{{ dato.stNumero }}</td>
                    <td>{{ dato.stNombre }}</td>
                    <td>{{ dato.precio }}</td>
                </tr>
                {% endfor %}
            </tbody>
        </table>
        {% if entities.datos.getTotalItemCount > 10 %}
            {{ knp_pagination_render(entities.datos) }}
        {% endif %}
{% endblock %}