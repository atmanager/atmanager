{% extends 'AtBundle::base_at.html.twig' %} 
{% block content %}

    <h5>Datos de falla agregada en AT: {{ entity.at.id }}</h5>

    <table class="table">
        <tbody>
            <tr>
                <th>Falla</th>
                <td>{{ entity.falla }}</td>
            </tr>
            <tr>
                <th>Jerarquia</th>
                <td>{{ entity.rol }}</td>
            </tr>
            
        </tbody>
    </table>

     <a href="{{path('at_falla', {'idAt': entity.at.id }) }}"><h4>Aceptar</h4></a>
    
{% endblock %}
