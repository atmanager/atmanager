{% extends 'BackendBundle::base_backend.html.twig' %}

{% block content %}
    <h3>Gesti√≥n de Prioridades</h3>
     <a href="{{ path('prioridad_new') }}">
    	<h4>[Crear Prioridad]</h4>
     </a>
     <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th colspan="3" style="text-align:center">Acciones</th>
                <th>Id</th>
                <th>Nombre</th>                      
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('prioridad_show', { 'id': entity.id }) }}">Ver</a></td>
                <td><a href="{{ path('prioridad_edit', { 'id': entity.id }) }}">Editar</a></td>
                <td><a href="{{ path('prioridad_eliminar', { 'id': entity.id }) }}">Eliminar</a></td>
                <td>{{ entity.id }}</td>
                <td>{{ entity.nombre }}</td>                
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
