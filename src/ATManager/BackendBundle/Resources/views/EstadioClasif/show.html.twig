{% extends 'BackendBundle::base_backend.html.twig' %}

{% block content %}
    <h3>Visualizar Clasificacion de estadios</h3>

    <table class="table table-bordered table-striped">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Inicializa AT</th>
                <td>{{ entity.iniciaAt }}</td>
            </tr>
            <tr>
                <th>Finaliza AT</th>
                <td>{{ entity.finalizaAt }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('estadioclasif') }}">
            Retornar a listado
        </a>
        <a href="{{ path('estadioclasif_edit', { 'id': entity.id }) }}">
            Editar
        </a>
      
    </li>
   
       
</ul>
{% endblock %}
